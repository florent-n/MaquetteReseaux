#!/bin/bash
i=$1
echo "
$(($((i*10))+1)):0/1 $(($((i*10))+2)):0/1
$(($((i*10))+1)):0/0 $(($((i*10))+3)):0/2
$(($((i*10))+1)):0/2 $(($((i*10))+8)):0/1
$(($((i*10))+2)):0/0 $(($((i*10))+3)):0/3
$(($((i*10))+2)):0/2 $(($((i*10))+9)):0/1
$(($((i*10))+3)):0/0 $(($((i*10))+5)):0/0
$(($((i*10))+3)):0/1 $(($((i*10))+4)):0/0
$(($((i*10))+8)):0/0 $(($((i*10))+9)):0/0

$(($((i*10))+4)):0/1@HOSTNAME_VAR 1004:1/0@HOSTNAME_VAR
$(($((i*10))+5)):0/1@HOSTNAME_VAR 1005:1/0@HOSTNAME_VAR
"
